xxx:	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#1-40*8*3,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#1-40*8*3,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#1-40*8*3,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#40*8,d4
	bsr	do1
	addi	#1-40*8*3,d4
	subq	#1,xc(a5)
	bne	xxx
	addi	#40*32-4*4,d4
	subq	#1,yc(a5)
	bne	yyy
here:	nop
	bra	closestuff
do1:	moveq	#8,d7
do1b:	move.l	(a4)+,d6
	moveq	#8,d5
do1a:	roxl.l	#1,d6
	roxl	#1,d3
	roxl.l	#1,d6
	roxl	#1,d2
	roxl.l	#1,d6
	roxl	#1,d1
	roxl.l	#1,d6
	roxl	#1,d0
	subq	#1,d5
	bne.s	do1a
	move.l	planes(a5),a0
	adda	d4,a0
	move.b	d0,(a0)
	move.l	planes+4(a5),a0
	adda	d4,a0
	move.b	d1,(a0)
	move.l	planes+8(a5),a0
	adda	d4,a0
	move.b	d2,(a0)
	move.l	planes+12(a5),a0
	adda	d4,a0
	move.b	d3,(a0)
	addi	#40,d4
	subq	#1,d7
	bne	do1b
	subi	#40*8,d4
	rts


loop:	bsr	waitevent
process:	bsr	getevent
	beq.s	loop
	bsr	processevent
	bra.s	loop



abort:	bsr	closestuff
	rts

waitevent:	move.l	execbase(a5),a6
	move.l	uport(a5),a0
	jmp	exwaitport(a6)
getevent:	move.l	uport(a5),a0
	move.l	execbase(a5),a6
	jsr	exgetmsg(a6)
	tst.l	d0
	beq.s	noevent
	move.l	d0,a1
	move	im_class+2(a1),d2
	move	im_code(a1),d3
	move	im_qualifier(a1),d6
	moveq	#0,d4
	moveq	#0,d5
	move	im_mousex(a1),d4
	move	im_mousey(a1),d5
	jsr	exreplymsg(a6)
	moveq	#1,d0
noevent:	rts


openstuff:	move.l	intbase(a5),a6
	lea	newscreen(pc),a0
	jsr	intopenscreen(a6)
	move.l	d0,screen(a5)
	beq	badopen
	lea	newwindow(pc),a0
	move.l	d0,nw_screen(a0)
	jsr	intopenwindow(a6)
	move.l	d0,window(a5)
	beq	badopen
	move.l	#$20000,d0
	moveq	#memf_public,d1
	move.l	execbase(a5),a6
	jsr	exallocmem(a6)
	move.l	d0,array(a5)
	move.l	#$20*2048,d0
	moveq	#memf_chip,d1
	jsr	exallocmem(a6)
	move.l	d0,tiles(a5)
badopen:	rts

checkstuff:	tst.l	screen(a5)
	beq.s	checkbad
	tst.l	window(a5)
	beq.s	checkbad
	tst.l	tiles(a5)
	beq.s	checkbad
	tst.l	array(a5)
	beq.s	checkbad
	moveq	#1,d0
checkbad:	rts


closestuff:	move.l	intbase(a5),a6
	move.l	window(a5),d0
	beq.s	cs1
	move.l	d0,a0
	clr.l	window(a5)
	jsr	intclosewindow(a6)
cs1:	move.l	screen(a5),d0
	beq.s	cs2
	move.l	d0,a0
	clr.l	screen(a5)
	jsr	intclosescreen(a6)
cs2:	move.l	execbase(a5),a6
	move.l	array(a5),d0
	beq.s	cs3
	move.l	d0,a1
	move.l	#$20000,d0
	jsr	exfreemem(a6)
cs3:	move.l	tiles(a5),d0
	beq.s	cs4
	move.l	d0,a1
	move.l	#$20*2048,d0
	jsr	exfreemem(a6)
cs4:	rts

loadlevel:	move.l	ipoint(a5),d1
	move.l	#1005,d2
	move.l	dosbase(a5),a6
	jsr	dosopen(a6)
	move.l	d0,d4
	beq.s	noloadlevel
	lea	level(a5),a0
	move.l	a0,d2
	move.l	d4,d1
	move.l	#l_size,d3
	jsr	dosread(a6)
	move.l	d4,d1
	jsr	dosclose(a6)
noloadlevel:	bra	showlevel

savelevel:	move.l	ipoint(a5),d1
	move.l	#1006,d2
	move.l	dosbase(a5),a6
	jsr	dosopen(a6)
	move.l	d0,d4
	beq.s	nosavelevel
	lea	level(a5),a0
	move.l	a0,d2
	move.l	d4,d1
	move.l	#l_size,d3
	jsr	doswrite(a6)
	move.l	d4,d1
	jsr	dosclose(a6)
nosavelevel:	rts


showlevel:	lea	level(a5),a4
	lea	tilelist(pc),a3
	adda	l_tileoff(a4),a3
	moveq	#0,d5
	moveq	#13,d6
puttypes:	bsr	showsquare
	addq	#1,a3
	addq	#3,d5
	subq	#1,d6
	bne.s	puttypes

	move	l_xpos(a4),d4
	move	l_ypos(a4),d5
	moveq	#edge,d0
	mulu	d5,d0
	add	d4,d0
	add	l_floor(a4),d0
	lea	l_levels(a4),a3
	adda	d0,a3
	move	#4*8*40,d5
	moveq	#7,d7
sly:	moveq	#13,d6
slx:	bsr	showsquare
	addq	#1,a3
	addq	#3,d5
	subq	#1,d6
	bne.s	slx
	adda	#edge-13,a3
	addi	#40*8*3-3*13,d5
	subq	#1,d7
	bne.s	sly
	rts
showsquare:	moveq	#0,d0
	move.b	(a3),d0
	add	d0,d0
	lea	squaremap(pc),a0
	adda	d0,a0
	move	(a0),d0
	ror	#8,d0
	add.l	d0,d0
	move.l	array(a5),a2
	adda.l	d0,a2
	move	(a2),d0
	bsr.s	show1
	move	2(a2),d0
	bsr.s	show1
	move	4(a2),d0
	bsr.s	show1
	addi	#8*40-3,d5
	move	512(a2),d0
	bsr.s	show1
	move	514(a2),d0
	bsr.s	show1
	move	516(a2),d0
	bsr.s	show1
	addi	#8*40-3,d5
	move	1024(a2),d0
	bsr.s	show1
	move	1026(a2),d0
	bsr.s	show1
	move	1028(a2),d0
	bsr.s	show1
	addi	#-2*8*40-3,d5
	rts
show1:	andi.l	#$7ff,d0
	lsl.l	#5,d0
	move.l	tiles(a5),a0
	adda.l	d0,a0
	move.l	planes(a5),a1
	bsr.s	showplane
	move.l	planes+4(a5),a1
	bsr.s	showplane
	move.l	planes+8(a5),a1
	bsr.s	showplane
	move.l	planes+12(a5),a1
	bsr.s	showplane
	addq	#1,d5
	rts
showplane:	adda	d5,a1
	move.b	(a0)+,(a1)
	move.b	(a0)+,40(a1)
	move.b	(a0)+,80(a1)
	move.b	(a0)+,120(a1)
	move.b	(a0)+,160(a1)
	move.b	(a0)+,200(a1)
	move.b	(a0)+,240(a1)
	move.b	(a0)+,280(a1)
	rts





initstuff:	move.l	window(a5),a0
	move.l	wd_userport(a0),uport(a5)
	move.l	screen(a5),a0
	lea	sc_bitmap+bm_planes(a0),a0
	lea	planes(a5),a1
	move.l	(a0)+,(a1)+
	move.l	(a0)+,(a1)+
	move.l	(a0)+,(a1)+
	move.l	(a0)+,(a1)+
	move	#-1,button(a5)


	move.l	dosbase(a5),a6
	lea	arrayname(pc),a0
	move.l	a0,d1
	move.l	#1005,d2
	jsr	dosopen(a6)
	move.l	d0,d4
	beq	noarray
	move.l	d4,d1
	move.l	array(a5),d2
	move.l	#$20000,d3
	jsr	dosread(a6)
	move.l	d4,d1
	lea	colortable(a5),a0
	move.l	a0,d2
	move.l	#128,d3
	jsr	dosread(a6)
	move.l	d4,d1
	lea	tilenum(a5),a0
	move.l	a0,d2
	moveq	#2,d3
	jsr	dosread(a6)
	move	tilenum(a5),d3
	lsl.l	#5,d3
	move.l	d4,d1
	move.l	tiles(a5),d2
	jsr	dosread(a6)
	move.l	d4,d1
	jsr	dosclose(a6)
	move.l	gfxbase(a5),a6
	lea	colortable(a5),a1
	move.l	screen(a5),a0
	lea	sc_viewport(a0),a0
	moveq	#16,d0
	jsr	gfxloadrgb4(a6)
noarray:	rts

processevent:	lea	level(a5),a4
	cmpi	#rawkey,d2
	beq	arawkey
	cmpi	#mousebuttons,d2
	beq	amousebutton
	cmpi	#mousemove,d2
	beq	amousemove
	rts
arawkey:	cmpi	#$45,d3
	beq	anescape
	cmpi	#$4c,d3
	beq	scrollup
	cmpi	#$4f,d3
	beq	scrollleft
	cmpi	#$4d,d3
	beq	scrolldown
	cmpi	#$4e,d3
	beq	scrollright
	cmpi	#$21,d3
	beq	savelevel
	cmpi	#$28,d3
	beq	loadlevel
	cmpi	#$46,d3
	beq	typesleft
	cmpi	#$5f,d3
	beq	typesright
	rts
anescape:	addq	#4,a7
	bra	abort
typesleft:	tst	l_tileoff(a4)
	beq	ignore
	subq	#1,l_tileoff(a4)
	bra	showlevel
typesright:	cmpi	#numtypes-13,l_tileoff(a4)
	beq	ignore
	addq	#1,l_tileoff(a4)
	bra	showlevel

scrollup:	tst	l_ypos(a4)
	beq	ignore
	subq	#1,l_ypos(a4)
	bra	allscroll
scrollleft:	tst	l_xpos(a4)
	beq	ignore
	subq	#1,l_xpos(a4)
	bra	allscroll
scrolldown:	cmpi	#edge-7,l_ypos(a4)
	beq	ignore
	addq	#1,l_ypos(a4)
	bra	allscroll
scrollright:	cmpi	#edge-13,l_xpos(a4)
	beq	ignore
	addq	#1,l_xpos(a4)
allscroll:	tst.b	button+1(a5)
	bmi	showlevel
	cmpi	#32,d5
	bcs	showlevel
	bra.s	amousemove

amousebutton:	move	d3,button(a5)
amousemove:	move	button(a5),d0
	tst.b	d0
	bmi	ignore
	cmpi	#24,d5
	bcs	changetype
	cmpi	#32,d5
	bcs	ignore
	cmpi	#iecode_lbutton,d0
	beq	paint
	clr	d0
	bra	paintany
ignore:	rts
;	cmpi	#iecode_lbutton,d3
;	beq	lbd
;	rts
;lbd:	cmpi	#24,d5
;	bcs	changetype
;	cmpi	#32,d5
;	bcc	paint
;	rts
changetype:	divu	#24,d4
	add	l_tileoff(a4),d4
	lea	tilelist(pc),a0
	move.b	0(a0,d4),d4
	move	d4,l_tile(a4)
	bra	showlevel
paint:	move	l_tile(a4),d0
paintany:	divu	#24,d4
	subi	#32,d5
	divu	#24,d5
	add	l_xpos(a4),d4
	add	l_ypos(a4),d5
	mulu	#edge,d5
	add	d5,d4
	add	l_floor(a4),d4
	lea	l_levels(a4),a0
	adda	d4,a0
	bsr.s	fixwall2
;	tst.b	d0
;	bpl.s	notwall
;	cmpi.b	#$c0,d0
;	bcc.s	notwall
;	bsr	compwall
;notwall:
	cmp.b	(a0),d0
	beq	ignore
	move.b	d0,(a0)
	lea	-edge(a0),a0
	bsr.s	fixwall
	lea	edge+1(a0),a0
	bsr.s	fixwall
	lea	edge-1(a0),a0
	bsr.s	fixwall
	lea	-edge-1(a0),a0
	bsr.s	fixwall
	bra	showlevel
fixwall:	move.b	(a0),d0
	bsr.s	fixwall2
	move.b	d0,(a0)
	rts
fixwall2:	tst.b	d0
	bpl	ignore
	cmpi.b	#$c0,d0
	bcc	ignore
	andi	#$f0,d0
compwall:	move	d0,d1
	clr	d2
	move.b	-edge(a0),d3
	bsr.s	wallbit
	move.b	1(a0),d3
	bsr.s	wallbit
	move.b	edge(a0),d3
	bsr.s	wallbit
	move.b	-1(a0),d3
wallbit:	andi	#$f0,d3
	eor	d1,d3
	bne.s	nosetwallbit
	bset	d2,d0
nosetwallbit:	addq	#1,d2
	rts





