	move.l	4.w,a6
	move.l	#workspace+$8000,d0
	move.l	#memf_public+memf_clear,d1
	jsr	exallocmem(a6)
	tst.l	d0
	beq	nomem
	move.l	d0,a5
	suba	#$8000,a5

	move.l	a6,execbase(a5)

	move.l	a2,ipoint(a5)
