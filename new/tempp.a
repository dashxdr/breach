	move.l	intbase(a5),a1
	jsr	excloselibrary(a6)
	move.l	gfxbase(a5),a1
	jsr	excloselibrary(a6)
	adda	#$8000,a5
	move.l	a5,a1
	move.l	#workspace+$8000,d0
	jsr	exfreemem(a6)

nomem:	rts

do:	bsr	openstuff
	bsr	checkstuff
	beq	abort
	bsr	initstuff
;	bsr	loadlevel

	lea	data(pc),a4
	moveq	#0,d4
	move	#4,yc(a5)
